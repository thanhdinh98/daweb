{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": "airbnb-base",
    "rules": {
        "no-restricted-globals": 0,
        "linebreak-style": "off",
        "import/prefer-default-export": 0,
        "no-restricted-syntax": 0
    }
}